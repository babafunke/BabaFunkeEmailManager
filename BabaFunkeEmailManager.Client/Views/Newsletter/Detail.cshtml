@model Newsletter

<div>
    <h2>Newsletter Detail</h2>

    <p></p>
    <div class="col-md-9">
        <div>
            <label class="col-md-3" asp-for="@Model.NewsletterId"></label>
            <input class="col-md-6" asp-for="@Model.NewsletterId" />
        </div>
        <div>
            <label class="col-md-3" asp-for="@Model.Subject"></label>
            <input class="col-md-6" asp-for="@Model.Subject" />
        </div>
        <div>
            <label class="col-md-3" asp-for="@Model.Body"></label>
            <textarea class="col-md-6" asp-for="@Model.Body"></textarea>
        </div>
        <div>
            <label class="col-md-3" asp-for="@Model.CreatedOn"></label>
            <input class="col-md-6" asp-for="@Model.CreatedOn" />
        </div>
        <div>
            <label class="col-md-3" asp-for="@Model.IsEnabled"></label>
            <input type="checkbox" class="col-md-6" asp-for="@Model.IsEnabled" />
        </div>

    </div>
</div>
